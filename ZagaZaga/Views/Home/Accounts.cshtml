    <script type="text/javascript">
        $(function () {
            $(".dropdown").hover(
                    function () {
                        $('.dropdown-menu', this).stop(true, true).fadeIn("fast");
                        $(this).toggleClass('open');
                        $('b', this).toggleClass("caret caret-up");
                    },
                    function () {
                        $('.dropdown-menu', this).stop(true, true).fadeOut("fast");
                        $(this).toggleClass('open');
                        $('b', this).toggleClass("caret caret-up");
                    });
        });


        $(document).ready(function () {

            $('#Bank_table').DataTable();
        });
        $(document).ready(function () {
            $('#Shipping_table').DataTable();
        });
        
        $(document).ready(function () {
            $('#Dating_table').DataTable();
        });
        
        $(document).ready(function () {
            $('#Craiglist_table').DataTable();
        });
        

    </script>
    <script>

        $('#myTabs a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
        })

    </script>
</head>

<body>

    <div class="header">
        <div class="top">
            <div class="container">
                <div class="row">

                    <div class="col-md-6 col-sm-6">
                        <p style="font-size:14px;color:#fff;">WELCOME TO THE <span style="color:#cc997f;font-weight:bold">ZAGA ZAGA</span></p>
                    </div><!--close col-->
                    <div class="col-md-6 col-sm-6">
                        <ul style="color:#fff" class="list-inline pull-right">
                            <li><a style="background-color:#eeeeee; outline:none; color:#000; font-weight:700" href="/AddMoney/Index" name="add_balance">Add Money</a></li>
                            <li>Your balance : $@Session["user_amount"]</li>
                            <li style="border-left:#AFAFAF 1px solid;border-right:#AFAFAF 1px solid;"><i class="fa fa-envelope" aria-hidden="true"></i></li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">@Session["user_name"] <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Profile</a></li>
                                    <li><a href="#">My Stuff</a></li>
                                    <li><a href="#">My Paypal</a></li>
                                    <li><a href="#">My Bulk</a></li>
                                    <li class="divider"></li>
                                    <li><a href="#">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div><!--close col-->
                </div><!--close row-->
            </div><!--close container-->
        </div><!--close top-->
        <nav class="navbar navbar-default">
            <div class="container">
                <div class="navbar-header">
                    <a href="@Url.Action("Index","Home")"><img src="~/Content/External/assets-1/img/logo.png"></a>
                </div>
                <ul class="nav navbar-nav">
                    <li><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li><a href="@Url.Action("Accounts","Home")" class="active-menu">Accounts</a></li>
                    <li><a href="@Url.Action("Index","Tools")">Tools</a></li>
                    <li><a data-toggle="modal" data-target="#myModal" href="#">Send Check Next Day Delivery</a></li>
                    <li><a href="@Url.Action("Index","Format")">Format & Write UPS</a></li>
                    <li><a href="@Url.Action("Transfers","Home")">Transfers</a></li>

                    <li><a href="@Url.Action("Index","User_Profile")">User Profile</a></li>
                    <li><a href="@Url.Action("Support_Dashboard", "Support")">Support</a></li>
                </ul>
            </div>
        </nav>


        <div class="slider">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <h1 class="b_icon"><i class="fa fa-university" aria-hidden="true"></i></h1>    
                    </div><!--close col-->
                </div><!--close row-->

                <div class="sld-inner">
                    <div class="row">

                        <div class="col-md-12 col-sm-12">
                            <div>

                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs" role="tablist">
                                    <li role="presentation" class="active"><a href="#Bank" aria-controls="Bank" role="tab" data-toggle="tab">
                                                                               <label style="background-color:red" class="lab">0</label>
                                        <span style="padding:5px; padding-right:10px"><i class="fa fa-money"></i></span>Bank Accounts</a></li>
                                    <li role="presentation"><a href="#Dating" aria-controls="Dating" role="tab" data-toggle="tab">
                                                                <label style="background-color:red" class="lab">0</label>
                                        <span style="padding:5px; padding-right:10px"><i class="fa fa-sitemap"></i></span>Dating Sites</a></li>
                                    <li role="presentation"><a href="#Shipping" aria-controls="Shipping" role="tab" data-toggle="tab">
                                                                <label style="background-color:red" class="lab">0</label>
                                        <span style="padding:5px; padding-right:10px"><i class="fa fa-ship"></i></span>Shipping Accounts</a></li>
                                    <li role="presentation"><a href="#Craiglist" aria-controls="Craiglist" role="tab" data-toggle="tab">
                                                                <label style="background-color:red" class="lab">0</label>
                                        <span style="padding:5px; padding-right:10px"><i class="fa fa-list-alt"></i></span>Craiglist Accounts</a></li>


                                    


                                </ul>

                                <!-- Tab panes -->
                                <div class="tab-content">

                                    <div role="tabpanel" class="tab-pane" id="profile">456</div>
                                    <div role="tabpanel" class="tab-pane" id="messages">789</div>
                                    <div role="tabpanel" class="tab-pane" id="settings">111</div>
                                </div>

                            </div>
                        </div><!--close col-->




                    </div><!--close row-->
                </div><!--close sld-inner-->

            </div><!--close container-->
        </div><!--close slider-->
    </div><!--close header-->

    <div class="section">
        <div class="sec-2">
            <div class="container">

                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="Bank">
                        <div class="col-md-12 col-sm-12">
                            <p style="font-size:18px; color:#00385c; font-weight:bold">Bank Accounts</p>
                            <table id="Bank_table" class="display" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Bank Name</th>
                                        <th>Country</th>
                                        <th>User Name</th>
                                        <th>User Pass</th>
                                        <th>Email	</th>
                                        <th>Password</th>
                                        <th>Balance</th>
                                        <th>Account Number</th>

                                        <th>Routine Number</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Bank Name</th>
                                        <th>Country</th>
                                        <th>User Name</th>
                                        <th>User Pass</th>
                                        <th>Email	</th>
                                        <th>Password</th>
                                        <th>Balance</th>
                                        <th>Account Number</th>
                                        <th>Routine Number</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </tfoot>
                                <tbody>
                                    @foreach (var item in ViewBag.bank)
                                    {
                                        <tr>
                                            <td>@item.bank_name</td>
                                            <td>@item.country</td>
                                            <td>@item.username</td>
                                            <td>@item.userpass</td>
                                            <td>@item.email</td>
                                            <td>@item.pass</td>
                                            <td>@item.balance</td>
                                            <td>@item.account_number</td>
                                            <td>@item.routine_number</td>
                                            <td>@item.price</td>
                                            <td>
                                                <button type="button" class="btn btn-success">BUY</button>
                                                @*<button type="button" class="btn btn-danger">NO CHECK!</button>*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div role="tabpanel" class="tab-pane" id="Shipping">
                        <div class="col-md-12 col-sm-12">
                            <p style="font-size:18px; color:#00385c; font-weight:bold">Shipping Accounts</p>
                            <table id="Shipping_table" class="display" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Additional Info</th>
                                        <th>Account Status</th>
                                        <th>User Name</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Additional Info</th>
                                        <th>Account Status</th>
                                        <th>User Name</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>

                                </tfoot>
                                <tbody>
                                    @foreach (var item in ViewBag.ship)
                                    {

                                        <tr>
                                            <td>@item.product_name</td>
                                            <td>@item.country</td>
                                            <td>@item.additional_info</td>
                                            <td>@item.account_status</td>
                                            <td>@item.username  </td>
                                            <td>@item.pass</td>
                                            <td>@item.price</td>

                                            <td>
                                                <button type="button" class="btn btn-success">BUY</button>
                                                @*<button type="button" class="btn btn-danger">NO CHECK!</button>*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                  

                    <div role="tabpanel" class="tab-pane " id="Dating">
                        <div class="col-md-12 col-sm-12">
                            <p style="font-size:18px; color:#00385c; font-weight:bold">Dating Sites</p>
                            <table id="Dating_table" class="display" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Sex</th>
                                        <th>Subscription</th>
                                        <th>Age</th>
                                        <th>Additional Info</th>
                                        <th>User Name</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Sex</th>
                                        <th>Subscription</th>
                                        <th>Age</th>
                                        <th>Additional Info</th>
                                        <th>User Name</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </tfoot>
                                <tbody>
