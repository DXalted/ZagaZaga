                                        <tr>
                                            <td>@item.product_name</td>
                                            <td>@item.country</td>
                                            <td>@item.sex</td>
                                            <td>@item.subscription</td>
                                            <td>@item.age</td>
                                            <td>@item.additional_info</td>
                                            <td>@item.username</td>
                                            <td>@item.pass</td>
                                            <td>@item.price</td>

                                            <td>
                                                <button type="button" class="btn btn-success">BUY</button>
                                                @*<button type="button" class="btn btn-danger">NO CHECK!</button>*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                   
                    <div role="tabpanel" class="tab-pane " id="Craiglist">
                        <div class="col-md-12 col-sm-12">
                            <p style="font-size:18px; color:#00385c; font-weight:bold">Craiglist Accounts</p>
                            <table id="Craiglist_table" class="display" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Details</th>
                                        <th>Status</th>
                                        <th>User Name	</th>
                                        <th>User Pass</th>
                                        <th>Email</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <th>Product Name</th>
                                        <th>Country</th>
                                        <th>Details</th>
                                        <th>Status</th>
                                        <th>User Name	</th>
                                        <th>User Pass</th>
                                        <th>Email</th>
                                        <th>Password</th>
                                        <th>Price</th>
                                        <th>Action</th>
                                    </tr>
                                </tfoot>
                                <tbody>
                                    @foreach (var items in ViewBag.crg)
                                    {
                                        <tr>
                                            <td>@items.product_name</td>
                                            <td>@items.country</td>
                                            <td>@items.details</td>
                                            <td>@items.status</td>
                                            <td>@items.username</td>
                                            <td>@items.userpass</td>
                                            <td>@items.email</td>
                                            <td>@items.pass</td>
                                            <td>@items.price</td>

                                            <td>
                                                <button type="button" class="btn btn-success">BUY</button>
                                                @*<button type="button" class="btn btn-danger">NO CHECK!</button>*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>

                    
                </div>




            </div>
        </div>
    </div>






    <div class="footer">

        <div class="row">
            <div class="col-md-12 col-sm-12">

                <div class="foot">
                    <p style="font-size:18.62px;color:#FFFFFF;text-align:center;font-weight:bold">Connect with us</p>

                    <div class="social-icon">
                        <i class="fa fa-facebook-square" aria-hidden="true"></i>
                        <i class="fa fa-twitter-square" aria-hidden="true"></i>
                        <i class="fa fa-linkedin-square" aria-hidden="true"></i>
